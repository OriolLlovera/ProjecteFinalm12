  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>{% block title %}ExchangeIt{% endblock %}</title>

  {% block stylesheets %}

    <link href="{{ asset('public/backend/css/bootstrap.css') }}" rel="stylesheet">

